@model IEnumerable<Code.Models.Catagory>
@{
    ViewData["Title"] = "searchlist";
}


           
<table class="table-bordered table-striped">
    <thead>
        <tr>
            <th>ID No:</th>
            <th>Name</th>
            <th>Display Order</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count() == 0) 
        { 
            <tr>
                <td colspan="3">
                    not match data
                </td>
            </tr>
        }
            else
            {
            foreach(var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                            <td>@item.Displayorder</td>
                    </tr>
                }
               
            }
    </tbody>
</table>